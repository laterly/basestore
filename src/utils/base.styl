    margin: 0;
    padding: 0;
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
    font-weight: 400;
}

input,
select {
    outline: 0
}

li {
    list-style: none;
}

a,
a:hover {
    color: #2c2c2c;
    text-decoration: none
}

i {
    font-style: inherit
}

body {
    min-height: 100%;
    min-width: 320px;
    margin: 0 auto;
    max-width: 750px;
    font-size: 14px;
    background: #FDFBFC;
    -webkit-text-size-adjust: none;
}

html {
    -webkit-text-size-adjust: none;
    width: 100%;
    height:
        100%
}

ol,
ul {
    list-style: none
}

a {
    text-decoration: none
}

a:hover {
    text-decoration: underline
}

table {
    border-collapse: collapse;
    border-spacing: 0
}
input[type=button],input[type=text],input[type=password]{-webkit-appearance:none;outline:none}

/*flex布局专用*/

.flex {
    display: flex;
}

.flex1 {
    flex: 1
}

.flex2 {
    flex: 2
}

.flex3 {
    flex: 3
}

.flex-center {
    display: flex;
    align-items: center;
    justify-content: center;
}
.flex-left {
    display: flex;
    align-items: center;
    justify-content: left;
}
.flex-row {
    display: flex;
    flex-flow: row wrap;
}

.flex-column {
    display: flex;
    flex-flow: column wrap;
}

/*flex部分结束*/


/*字体加粗结束*/

/*display部分*/

.hidden {
    display: none;
}

.block {
    display: block !important;
}


/*display部分结束*/
/*图片自适应*/
.img-responsive{
    width: 100%;
    height: auto;
}

.img-block{
    width: 100%;
    height: 100%;
    object-fit: cover;
}

/*浮动*/

.pull-left {
    float: left;
}

.pull-right {
    float: right;
}

/*浮动结束*/

/*清除浮动*/

.clearfix {
    *zoom: 1;
    clear: both;
    overflow: hidden;
}

.clearfix:after {
    display: block;
    height: 0;
    overflow: hidden;
    content: " ";
    visibility: hidden;
    clear: both;
}

/*清除浮动结束*/

/*绝对定位*/

.rel {
    position: relative;
}

.abs {
    position: absolute
}

.fix {
    position: fixed;
}

/*绝对定位结束*/

/*文字超出隐藏*/

.text-elli {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

/*文字超出隐藏结束*/



@media screen and (max-width: 320px) {
    html {
        font-size:42.666666666667px
    }
}

@media screen and (min-width: 321px) and (max-width:375px) {
    html {
        font-size:50px
    }
}

@media screen and (min-width: 376px) and (max-width:414px) {
    html {
        font-size:55.2px
    }
}

@media screen and (min-width: 415px) and (max-width:480px) {
    html {
        font-size:64px
    }
}

@media screen and (min-width: 481px) and (max-width:540px) {
    html {
        font-size:72px
    }
}

@media screen and (min-width: 541px) and (max-width:640px) {
    html {
        font-size:85.3333333333333px
    }
}

@media screen and (min-width: 641px) and (max-width:720px) {
    html {
        font-size:96px
    }
}

@media screen and (min-width: 721px) and (max-width:750px) {
    html {
        font-size:100px
    }
}

@media screen and (min-width: 751px) {
    html {
        font-size:100px;
        width: 750px;
        margin: 0 auto
    }
}

.border { border:1px solid rgba(214,214,214,1); }
@media screen and (-webkit-min-device-pixel-ratio: 2) {
    .border { border: 0.5px solid rgba(214,214,214,1) }
}
@media screen and (-webkit-min-device-pixel-ratio: 3) {
    .border { border: 0.333333px solid rgba(214,214,214,1) }
}